{
  "overrides": [
    {
      "files": [
        "*.sol",
        "*.js",
        "*.test.js",
        "*.json",
        "*.test.ts"
      ],
      "options": {
        "printWidth": 99,
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": false,
        "bracketSpacing": true,
        "explicitTypes": "always"
      }
    }
  ]
}